define_primitive_function "", prepare_for
   ;; << -- >>
   pop_return_stack rbx
   push_return_stack pointer$argument_stack
   push_return_stack rbx
.next:
   add rbx, jo_size
   mov rax, qword [rbx]
   cmp rax, end_of_prepare
   je .then
   jmp .next
.then:
